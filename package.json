{
  "name": "knex-schemer",
  "version": "1.0.0",
  "description": "Schema Build and update tool for knex",
  "main": "lib/schemer.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhoriuchi/knex-schemer.git"
  },
  "keywords": [
    "knex",
    "schema",
    "builder",
    "sync"
  ],
  "author": "Branden Horiuchi <bhoriuchi@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bhoriuchi/knex-schemer",
  "dependencies": {
    "bluebird": "^3.4.0",
    "manakin": "^0.4.0"
  },
  "devDependencies": {
    "babel-register": "^6.11.0",
    "babel-runtime": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "6.13.0",
    "babel-preset-stage-2": "^6.0.0",
    "chai": "^3.5.0",
    "rollup": "^0.34.0",
    "rollup-plugin-babel": "^2.5.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "knex": "^0.11.0",
    "mocha": "^3.0.0",
    "mysql": "^2.11.0",
    "pg": "^6.0.0",
    "lodash": "^4.14.0"
  },
  "bugs": {
    "url": "https://github.com/bhoriuchi/knex-schemer/issues"
  },
  "scripts": {
    "build:copybrc": "cp build/.build.babelrc src/.babelrc",
    "build:rmbrc": "rm -f src/.babelrc",
    "build": "npm run build:copybrc && rollup -c build/rollup.config.js && npm run build:rmbrc",
    "test": "npm run build && mocha test/index.js",
    "test:nobuild": "mocha test/index.js"
  },
  "directories": {
    "example": "example"
  }
}
